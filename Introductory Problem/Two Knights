#include<bits/stdc++.h>
using namespace std;
#define int long long
 
int ok(int n)
{
    if(n == 1) return 0;
    int x=n*n;
    return x*(x-1)/2 - 4*(n-1)*(n-2);
}
 
int32_t main()
{
    int n;
    cin>>n;
    for(int i=1; i<=n; i++)
        cout<<ok(i)<<endl;
}
